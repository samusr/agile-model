const <%= association.rModel.name %> = require("../../../models/<%= association.rModel.filename %>");

module.exports = function(<%= association.lModelVarName %>, relationString = "") {
    return <%= association.rModel.name %>.query()
        .where({ <%= association.lModel.singular_tablename + "_id" %>: <%= association.lModelVarName %> })
        .eager(relationString)
        .returning("*");
};
